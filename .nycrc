{
  "check-coverage": true,
  "lines": 40,
  "functions": 40,
  "branches": 40,
  "statements": 40,
  "include": [
    "src/**/*"
  ],
  "exclude": [
    "build/",
    "spec/**/*",
    "src/platforms/!(shared)*/**/*"
  ],
  "extension": [
    ".ts"
  ],
  "reporter": [
    "html",
    "text",
    "clover"
  ],
  "report-dir": "spec/build/",
  "all": true,
  "cache": true,
  "sourceMap": false,
  "instrument": false
}
