{
	"extends": "../../tsconfig.misc.json",
	"compilerOptions": {
		"outDir": "./dist",
		"declarationDir": "./dist",
		"module": "commonjs",
		"target": "esnext",
		"allowJs": false,
		"sourceMap": false,
		"noImplicitReturns": true,
		"noImplicitThis": true,
		"noImplicitAny": true,
		"noUnusedLocals": true
	},
	"include": ["./src"]
}
