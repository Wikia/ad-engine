{
	"extends": "../tslint.json",
	"rules": {
		"no-console": false,
		"no-unused-variable": false,
		"no-unused-expression": false,
		"no-implicit-dependencies": [
			true,
			"dev",
			[
				"@wikia/ad-bidders",
				"@wikia/ad-engine",
				"@wikia/ad-products",
				"@wikia/ad-services"
			]
		],
		"no-submodule-imports": false,
		"max-line-length": [true, { "limit": 100, "ignore-pattern": "^import |^export {(.*?)} |http |it" }]
	}
}
