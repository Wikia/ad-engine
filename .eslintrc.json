{
    "parser": "babel-eslint",
    "extends": "airbnb",
    "rules": {
        "class-methods-use-this": 0,
        "comma-dangle": 0,
        "func-names": 0,
        "import/prefer-default-export": 0,
        "import/extensions": 0,
        "import/no-extraneous-dependencies": 0,
        "import/no-unresolved": 0,
        "indent": [2, "tab", {"VariableDeclarator": 1, "SwitchCase": 1}],
        "max-len": [2, 120],
        "no-empty": 0,
        "no-mixed-operators": 0,
        "no-new": 0,
        "no-param-reassign": 0,
        "no-plusplus": 0,
        "no-use-before-define": 0,
        "no-tabs": 0,
        "one-var": 0
    },
    "globals": {
        "document": false,
        "QUnit": false,
        "window": false
    }
}
